{
  "compilerOptions": {
    "module": "es2015",
    "target": "ES6",
    "jsx": "react",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  }
}